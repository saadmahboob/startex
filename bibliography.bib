# if you find yourself needing to quote a website
@misc{startex,
    title="{startex}",
    url={https://github.com/xorbyte/startex/},
    howpublished="\url{https://github.com/xorbyte/startex/}",
}

# more than one is kinda pushing it though
# \\\url to force on the next line or we get ugly wrapping
@misc{latex-makefile,
    title="{latex-makefile}",
    url={http://code.google.com/p/latex-makefile/},
    howpublished="\\\url{http://code.google.com/p/latex-makefile/}",
}